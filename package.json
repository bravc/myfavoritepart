{
  "name": "myfavoritpart",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run-script build && node dist/server.js",
    "watch-ts": "tsc -w",
    "build": "npm run build-ts && npm run copy",
    "build-ts": "tsc",
    "watch-node": "nodemon dist/server.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy": "ts-node copyStaticAssets.ts",
    "addmodel": "ts-node utils.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-winston": "^2.0.0",
    "http-errors": "~1.6.2",
    "mysql2": "^1.5.3",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "winston": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/passport": "^0.4.3",
    "@types/passport-facebook": "^2.1.7",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.47.0",
    "db-migrate": "^0.11.1",
    "db-migrate-mysql": "^1.1.10",
    "express": "^4.16.3",
    "pug": "^2.0.3",
    "sequelize-cli": "^4.0.0",
    "shelljs": "^0.8.1",
    "ts-node": "^5.0.0"
  }
}
